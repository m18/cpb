{
    "messages": {
        "in": {
            "p(id, name)": {
                "name": "tutorial.Person",
                "template": {
                    "id": "$id",
                    "name": "$name"
                }
            }
        },
        "out": {
            "p": {
                "name": "tutorial.Person",
                "template": "Person: \"$name\", ($address.postcode) - amount: \\$5"
            }
        }
    }
}