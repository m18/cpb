{
    "messages": {
        "in": {
            "p(id, name, postcode, onestr, phones)": {
                "name": "tutorial.Person",
                "template": {
                    "id": "$id",
                    "name": "$name",
                    "home_address": {
                        "postcode": "$postcode"
                    },
                    "onestr": "$onestr",
                    "phones": "$phones"
                }
            }
        },
        "out": {
            "p": {
                "name": "tutorial.Person",
                "template": "Person: \"$name\", ($home_address.postcode) : >$onestr< - amount: \\$5"
            }
        }
    }
}